{
  "tags": {
    "allUnknownTags": true
  },
  "plugins": ["./node_modules/jsdoc-export-default-interop/dist/index"], 
  "source": {
    "include": [ "src" ],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "templates": {
    "systemName": "Tata Sky Front-End Code Documentation",
    "cleverLinks": false,
    "disableSort": false,
    "collapse": true,
    "theme": "united",
    "dateFormat": "MMMM Do YYYY, h:mm:ss a",
    "default":{
      "outputSourceFiles": true
    }
  },
  "opts": {
    "destination": "./docs/jsdocs"
  }
}
